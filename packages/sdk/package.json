{
  "name": "@oss-ai-agent-tool/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for OSS AI Agent Tool",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "require": "./dist/client/index.js",
      "types": "./dist/client/index.d.ts"
    },
    "./tools": {
      "require": "./dist/tools/index.js",
      "types": "./dist/tools/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run test"
  },
  "keywords": [
    "ai",
    "agent",
    "llm",
    "orchestrator",
    "tools",
    "mcp",
    "sdk"
  ],
  "author": "OSS AI Agent Tool Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/oss-ai-agent-tool.git",
    "directory": "packages/sdk"
  },
  "bugs": {
    "url": "https://github.com/your-org/oss-ai-agent-tool/issues"
  },
  "homepage": "https://github.com/your-org/oss-ai-agent-tool#readme",
  "dependencies": {
    "zod": "^3.23.8",
    "undici": "^6.18.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "tsup": "^8.0.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.5"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
